<%= form_with model: @study, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "タイトル" %><br />
    <%= f.text_field :title, class: :form__text, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :catch_copy, "言語" %><br />
    <%= f.text_area :language, class: :form__text, id:"prototype_catch_copy" %>
  </div>

  <div class="field">
    <%= f.label :concept, "フレームワーク" %><br />
    <%= f.text_area :framework, class: :form__text, id:"prototype_concept" %>
  </div>

    <div class="field">
    <%= f.label :concept, "内容" %><br />
    <%= f.text_area :content, class: :form__content, id:"prototype_concept" %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>